vector<ll>v[100000];
bool visi[100000];
void dfs(ll a){
visi[a]=true;
//cout<<a<<" ";
for(ll i=0;i<v[a].size();i++){
    if(!visi[v[a][i]])dfs(v[a][i]);
}
}]]></content>
    <!-- Optional: Set a tabTrigger to define how to trigger the snippet -->
    <tabTrigger>dfs</tabTrigger>
