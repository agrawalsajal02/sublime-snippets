    <content><![CDATA[void per(string s,string r,ll i,ll n){
//cout<<r<<"  "<<i<<endl;
if(i==n){
    cout<<r<<endl;
    return;
}
    per(s,r,i+1,n);
    per(s,r+s[i],i+1,n);

}]]></content>
