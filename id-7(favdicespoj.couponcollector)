    for(ll i=0;i<t;i++){
ll n;
cin>>n;
double sum=0;
for(int j=1;j<=n;j++){
sum=sum+n/(j*1.0);
}


cout<<fixed<<setprecision(6)<<sum<<endl;





    }
}
