    <content><![CDATA[
ll stringToInt(string a,ll m){
    
    ll ans = 0;
    for(int i=0;i<a.size();i++){
        ans = (ans*10)%m + (a[i]-'0');
        ans %= m;
    }
    return ans;
}
]]></content>
